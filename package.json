{
  "name": "@e0ipso/ai-task-manager",
  "version": "0.1.0",
  "description": "AI-powered task management CLI tool",
  "main": "dist/index.js",
  "bin": {
    "ai-task-manager": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "commitlint": "commitlint --edit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "cli",
    "task-management",
    "ai",
    "npx",
    "typescript"
  ],
  "author": "e0ipso",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "templates/",
    "commands/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e0ipso/ai-task-manager.git"
  },
  "bugs": {
    "url": "https://github.com/e0ipso/ai-task-manager/issues"
  },
  "homepage": "https://github.com/e0ipso/ai-task-manager#readme"
}
